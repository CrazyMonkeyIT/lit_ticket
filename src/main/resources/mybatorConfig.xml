        </javaModelGenerator>  
          
        <!--对应的xml mapper文件  -->  
        <sqlMapGenerator targetPackage="cn.zjtx.report.dao" targetProject="shzjtx">  
            <property name="enableSubPackages" value="true"/>  
        </sqlMapGenerator>  
          
        <!-- 对应的dao接口 -->  
        <javaClientGenerator type="XMLMAPPER" targetPackage="cn.zjtx.report.dao" targetProject="shzjtx">  
            <property name="enableSubPackages" value="true"/>  
        </javaClientGenerator>  
        
